{
	"name": "node-js-api",
	"version": "1.0.0",
	"description": "ATM API",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"start": "bun src/index.ts",
		"build": "bun build ./src/index.ts --outdir ./dist",
		"dev": "bun --hot src/index.ts",
		"format": "bun biome format --write .",
		"lint": "bun biome lint --write .",
		"lint:check": "bun biome check .",
		"ci:check": "bun biome ci .",
		"test": "bun run test:unit && bun run test:integration",
		"test:unit": "bun test src/modules",
		"test:integration": "bun test test/integration"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@types/bun": "latest",
		"@types/express": "^4.17.21",
		"@types/iban": "^0.0.35"
	},
	"dependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"iban": "^0.0.14",
		"pino": "^9.7.0",
		"pino-http": "^10.5.0",
		"pino-pretty": "^13.1.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.23.8"
	}
}
